{
  "name": "@laeri/config-generator",
  "author": {
    "name": "Will Laeri",
    "email": "wlaeri@gmail.com"
  },
  "license": "MIT",
  "version": "0.0.1",
  "description": "Command line utilities for generating typed configuration code.",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf ./node_modules ./lib ./coverage",
    "format": "prettier --write 'src/**/*.{ts,json}'",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "jest",
    "coverage": "jest --coverage",
    "tsc": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "prepublish": "yarn build",
    "build": "yarn clean && yarn tsc",
    "prepare": "yarn build && husky install",
    "prepack": "yarn build",
    "pack:local": "yarn pack && tar -xvf package.tgz",
    "generate": "yarn build && ./dist/bin generate --no-output",
    "init": "yarn build && ./dist/bin init"
  },
  "peerDependencies": {
    "@laeri/config-parser": "0.0.1"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "commander": "^9.4.1",
    "handlebars": "^4.7.7",
    "inquirer": "^9.1.2",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@types/inquirer": "^9",
    "@types/jest": "^29.1.1",
    "@types/node": "18.6.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "latest",
    "eslint": "8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "28.1.2",
    "prettier": "2.7.1",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
}
